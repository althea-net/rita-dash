{
  "name": "althea-dash",
  "productName": "althea",
  "version": "0.3.13",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "PORT=3005 react-scripts start",
    "build": "react-scripts build && rm build/static/**/*.map && rm -rf build/icons",
    "buildall": "react-scripts build",
    "package-android": "cordova build android --release",
    "package-ios": "cordova build ios --release --buildFlag=\"-useModernBuildSystem=0\"",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "homepage": ".",
  "cordova": {
    "plugins": {
      "cordova-plugin-add-swift-support": {},
      "cordova-plugin-whitelist": {},
      "cordova-plugin-customurlscheme": {
        "URL_SCHEME": "althea",
        "ANDROID_SCHEME": " ",
        "ANDROID_HOST": " ",
        "ANDROID_PATHPREFIX": "/"
      }
    },
    "platforms": [
      "android",
      "ios"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@craco/craco": "^5.6.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.2",
    "@fortawesome/free-solid-svg-icons": "^5.2.0",
    "@fortawesome/react-fontawesome": "0.1.0-11",
    "abortcontroller-polyfill": "^1.3.0",
    "ajv": "^6.5.2",
    "animated-scroll-to": "^1.2.2",
    "bignumber.js": "^7.2.1",
    "bootstrap": "^4.1.3",
    "camelcase-keys-deep": "0.1.0",
    "concurrently": "^4.0.1",
    "cordova": "^8.1.2",
    "cordova-android": "^7.1.1",
    "cordova-ios": "^5.0.1",
    "cordova-plugin-add-swift-support": "^1.7.2",
    "cordova-plugin-customurlscheme": "^4.4.0",
    "cordova-plugin-whitelist": "1",
    "core-util-is": "^1.0.2",
    "cross-env": "^5.2.0",
    "date-fns": "2.0.0-alpha.13",
    "debounce": "^1.2.0",
    "email-validator": "^2.0.4",
    "eslint": "5",
    "eslint-plugin-flowtype": "^3.7.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.12.4",
    "ethereum-address": "^0.0.4",
    "i": "^0.3.6",
    "i18next": "^14.0.1",
    "i18next-browser-languagedetector": "^2.2.3",
    "i18next-xhr-backend": "^1.5.1",
    "ip-address": "^5.8.9",
    "jquery": "^1.12.4",
    "js-sha3": "^0.8.0",
    "node-sass": "^4.11.0",
    "npm": "^6.3.0",
    "popper.js": "^1.14.4",
    "prettier": "1.14.0",
    "prop-types": "^15.6.2",
    "qrcode.react": "^0.8.0",
    "qrious": "^4.0.2",
    "react": "^16.11.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.8.1",
    "react-i18next": "^10.0.0",
    "react-phone-number-input": "^2.3.11",
    "react-scripts": "^3.0.1",
    "react-sigma": "^1.2.30",
    "react-stateful": "https://github.com/asoltys/react-stateful",
    "react-testing-library": "^5.9.0",
    "react-widgets": "4",
    "react-widgets-date-fns": "^4.0.26",
    "reactstrap": "^8.0.0",
    "source-map-explorer": "^2.0.1",
    "styled-components": "^3.4.9",
